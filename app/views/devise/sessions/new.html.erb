<body style="background-image:url(<%= asset_path '34.jpg'%>)">
    <div id="custom-bootstrap-menu" class="navbar navbar-default " role="navigation">
    <div class="container">
      <div class="navbar-header"><a class="navbar-brand" href="#">Mysociety</a>
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-menubuilder"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
          </button>
      </div>
      <div class="collapse navbar-collapse navbar-menubuilder">
        <ul class="nav navbar-nav navbar-right">
          <li><a href="/"></a>
          </li>
          <li><a href="/products"></a>
          </li>
          <li><a href="/about-us"> </a>
          </li>
          <li><a href="/contact"> </a>
          </li>
        </ul>
      </div>
    </div>
    </div>
    <div class="container">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 text-center">
        <div id="banner">
          <h1>Mysociety <strong>login Form</strong> </h1>
          <h5><strong></strong></h5>
        </div>            
      </div>
    <center>
    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
      <div class="registrationform">
        <h2>Log in</h2>
        <%= form_for(:user, :url => new_session_path(:user), :method => :post) do |f| %>
        <div class="field">
          <%= f.label :email %><br/>
          <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
        </div>
        <div class="field">
          <%= f.label :password %><br />
          <%= f.password_field :password, autocomplete: "off" %>
        </div>
        <% if devise_mapping.rememberable? -%>
        <div class="field">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
        <% end -%>
        <div class="action" >
          <%= f.submit "Log in" %>
       </div>
      <% end %>
      <%= render "devise/shared/links" %>
      </div>
       </div>
    </div>
    </center>   
  </body>
