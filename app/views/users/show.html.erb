<body  style="background-image:url(<%= asset_path '11.jpg'%>);background-size: cover">
	  <%= render partial:"layouts/navbar"%>
   <p>Name::<%= @user.name%></p>
   <p>Email::<%= @user.email%></p>
   <p>Date of Birth::<%= @user.dob%></p>
   <p>Society_name::<%= @user.society.society_name%></p>
   <p>Gender::<%= @user.gender%></p>
   <p><%= image_tag @user.image%></p>
   <% if current_user.has_role? "admin"%>
      <%= link_to "Send message ",   new_message_path(:message =>{:message_to => @user.id} ) %>
   <%end%>
</body>
